<% data.forEach(function(OnePagePost){%>
	<!-- Navigation -->
	<% include componentWeb/header %>
		<!-- Page Header -->
		<header class="masthead" style="background-image: url('/images/post-bg.jpg')">
			<div class="overlay"></div>
			<div class="container">
				<div class="row">
					<div class="col-lg-8 col-md-10 mx-auto">
						<div class="post-heading">
							<h1><%= OnePagePost.titleProduct %></h1>
							<h2 class="subheading"><%= OnePagePost.Description %></h2>
						</div>
					</div>
				</div>
			</div>
		</header>
		<!-- Post Content -->
		<article>
			<div class="container">
				<div class="row">
					<div class="col-lg-8 col-md-10 mx-auto">
						<img src="<%= OnePagePost.anh[0] %>" alt="" style="Width:700px; height:328px;">
						<p><%= OnePagePost.contentProduct %></p>
					
					</div>
				</div>
			</div>
		</article>
		<hr>
	<!-- Footer -->
	<% include componentWeb/footer %>
<% }); %>